{
  "build": {
    "content": [
      {
        "files": [
          "files/**.md",
          "files/toc.md",
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
	        "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.png",
          "**/*.gif",
          "**/*.html",
          "**/*.pdf",
          "**/*.xlsx",
          "**/*.pptx",
          "**/*.csv",
          "**/*.xml",
          "**/*.ttf",
          "**/*.webp",
          "**/*.ico",
          "**/*.mp4",
          "**/*.css",
          "**/*.js",
          "**/*.mp3",
          "**/*.json",
          "**/*.css",
          "**/*.hls",
          "**/*.m3u8",
          "**/*.ts",
          "**/*.svg",
          "**/*.txt",          
          "**/*.woff"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
	        "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "dest": "D:/Test/woocommerce-connector-help.github.io/docs",
    "template": [
      "default",
      "D:/Test/woocommerce-connector-help.github.io/Templates/SingulinkFX/singulinkfx"

    ],
    "postProcessors": [ "ExtractSearchIndex" ],
"globalMetadata": {
    "_appTitle": "WooCommerce Connector Help",
    "_appName": "",
